{
  "name": "tiny-toolkit",
  "version": "0.0.4",
  "description": "A tiny collection of useful javascript game dev tools",
  "main": "./dist/index.js",
  "files": [
    "dist/"
  ],
  "directories": {
    "example": "example"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sajmoni/tiny-toolkit.git"
  },
  "private": false,
  "prettier": {
    "trailingComma": "all",
    "semi": false,
    "singleQuote": true
  },
  "scripts": {
    "clean": "rm -f tiny-toolkit.tgz",
    "build-test": "./build-test.sh",
    "release": "yarn clean && yarn audit && yarn build && np --no-tests",
    "test": "ava",
    "lint": "eslint src/index.js",
    "build": "rm -rf dist && rollup --config"
  },
  "devDependencies": {
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "np": "6.2.0",
    "prettier": "1.19.1",
    "rollup": "2.0.6"
  }
}
