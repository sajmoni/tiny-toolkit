{
  "name": "tiny-toolkit",
  "version": "0.0.3",
  "description": "A tiny collection of useful javascript game dev tools",
  "main": "./dist/index.js",
  "files": [
    "dist/"
  ],
  "directories": {
    "example": "example"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sajmoni/tiny-toolkit.git"
  },
  "private": false,
  "scripts": {
    "clean": "rm -f tiny-toolkit.tgz",
    "build-test": "yarn clean && yarn build && yarn pack --filename tiny-toolkit.tgz && cd example && yarn refresh && yarn test && cd -",
    "release": "yarn build && np",
    "test": "ava",
    "lint": "eslint index.js",
    "build": "rm -rf dist && parcel build ./index.js --no-cache"
  },
  "devDependencies": {
    "@babel/preset-env": "7.8.4",
    "@babel/register": "7.8.3",
    "ava": "3.2.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.20.1",
    "np": "5.2.1",
    "parcel": "2.0.0-alpha.3.2"
  },
  "ava": {
    "require": [
      "@babel/register"
    ]
  }
}
