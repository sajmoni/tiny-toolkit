{
  "name": "tiny-toolkit",
  "version": "0.2.0",
  "description": "A tiny collection of useful javascript game dev tools",
  "main": "./dist/index.js",
  "files": [
    "dist/"
  ],
  "directories": {
    "example": "example"
  },
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sajmoni/tiny-toolkit.git"
  },
  "private": false,
  "ava": {
    "require": [
      "esbuild-runner/register"
    ],
    "extensions": [
      "ts"
    ]
  },
  "prettier": "prettier-config-one",
  "scripts": {
    "go": "npm run build && preview && ava",
    "release": "npm run build && np --no-tests",
    "test": "ava",
    "build": "rm -rf dist && tsc"
  },
  "devDependencies": {
    "ava": "4.3.1",
    "esbuild-runner": "2.2.1",
    "np": "7.6.2",
    "package-preview": "4.0.0",
    "prettier": "2.7.1",
    "prettier-config-one": "0.1.0",
    "remeda": "0.0.35",
    "tsconfig-one": "0.0.2",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "round-to": "5.0.0"
  }
}
