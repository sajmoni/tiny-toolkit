{
  "name": "tiny-toolkit",
  "version": "0.0.3",
  "description": "A tiny collection of useful javascript game dev tools",
  "main": "./dist/index.js",
  "files": [
    "dist/"
  ],
  "directories": {
    "example": "example"
  },
  "license": "MIT",
  "private": false,
  "scripts": {
    "clean": "rm -f tiny-toolkit.tgz",
    "build-test": "yarn clean && yarn build && yarn pack --filename tiny-toolkit.tgz && cd example && yarn refresh && yarn test && cd -",
    "release": "yarn build && np",
    "test": "ava",
    "lint": "eslint index.js",
    "build": "rm -rf dist && parcel build ./index.js --no-cache"
  },
  "devDependencies": {
    "@babel/preset-env": "7.6.2",
    "@babel/register": "7.6.2",
    "ava": "2.4.0",
    "eslint": "6.5.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.18.2",
    "np": "5.1.0",
    "parcel-bundler": "1.12.3"
  },
  "ava": {
    "require": [
      "@babel/register"
    ]
  }
}
